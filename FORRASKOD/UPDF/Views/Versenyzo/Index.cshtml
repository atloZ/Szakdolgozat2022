@model IEnumerable<UPDF.Models.Versenyzo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nev)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SzulHely)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SzulDatum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngedelySzam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngedelyErv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EgyesuletAzonNavigation.Nev)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nev)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SzulHely)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SzulDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngedelySzam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngedelyErv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EgyesuletAzonNavigation.Nev)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SirAzon">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SirAzon">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SirAzon">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
